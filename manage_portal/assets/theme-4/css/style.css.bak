    width: 100%;
}
.bottom-btn-row ul li .bg-color-v3{
    color: #fff;
}
.hr-text-editor{
    float: left;
    width: 100%;
}
.hr-text-editor p{
}
.comapny-video{
    float: left;
    width: 100%;
    position: relative;
}
.comapny-video:after {
    background-color: #000;
    content: "";
    height: 100%;
    left: 0;
    opacity: 0.4;
    position: absolute;
    top: 0;
    width: 100%;
}
.comapny-video iframe{
    border:none;
