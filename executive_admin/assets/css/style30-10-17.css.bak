@media (min-width:768px) and (max-width:1024px){
	.month-name {
	    border-radius: 5px 5px 0 0;
	    font-weight: 600;
	    margin: 0 0 25px;
	    text-align: center;
	}
	.month-name:after {
	    bottom: -10px;
	    margin-right: -10px;
	    right: 50%;
	    top: auto;
	}
}
@media (max-width:767px){
	.field-row {
	    margin: 7px 0;
	}
	.field-row {
	    height: auto;
	}
	.valign-middle {
	    line-height: normal;
	    margin: 0 0 5px !important;
	}
	.transparent-label {
	    display: none;
	}
	.table-responsive > .table-bordered {
	    border: 1px solid #ddd !important;
	}
	.table-responsive{
		border:none !important;
		margin: 0 !important;
	}
	.heading-title {
	    text-align: center;
	}
	.heading-title .page-title,
	.heading-title .black-btn {
	    display: inline-block;
	    float: none !important;
	    margin: 5px 0;
	}
	.logo, .topRight {
	    text-align: center;
	    width: 100%;
	}
	.month-name {
	    border-radius: 5px 5px 0 0;
	    font-weight: 600;
	    margin: 0 0 25px;
	    text-align: center;
	}
	.month-name:after {
	    bottom: -10px;
	    margin-right: -10px;
	    right: 50%;
	    top: auto;
	}
}
