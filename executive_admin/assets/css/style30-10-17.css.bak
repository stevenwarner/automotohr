    content: "\f107";
    position: absolute;
    top: 0;
    right: 0;
    width: 25px;
    height: 40px;
    color: #000;
    font-size: 16px;
    cursor: pointer;
    font-family: 'FontAwesome';
    text-align: center;
    line-height: 40px;
    pointer-events: none;
}
