}

.future_opportunity {
    background-color: #00008B;
}

.left_message{
    background-color: #B2B200;
}

.emp-info-strip{
    float: left;
    width: 100%;
    padding: 20px 0;
    position: relative;
    background: url(../images/ap-bg.jpg);
    border-bottom:2px dotted #ababab;
    margin: 0 0 20px 0;
}
.emp-info-box{
    float: left;
    width: 100%;
    position: relative;
}
.emp-info-box .figure{
    float: left;
    width: 190px;
    height: 143px;
    position: relative;
    /*background-color: #0061a4;*/
    text-align: center;
    padding: 15px;
}
.emp-info-box .figure img{
    position: absolute;
    top: 50%;
    left: 50%;
    width: 85%;
    height: 85%;
    -webkit-transform: translate(-50%, -50%);
    -moz-transform: translate(-50%, -50%);
    -ms-transform: translate(-50%, -50%);
    -o-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
}
.emp-info-box .figure span{
    position: absolute;
    top: 50%;
    left: 50%;
    font-size: 50px;
    color: #fff;
    width: 100%;
    height: 100%;
    line-height: 140px;
    background-color: #81b431;
    text-transform: uppercase;
    -webkit-transform: translate(-50%, -50%);
    -moz-transform: translate(-50%, -50%);
    -ms-transform: translate(-50%, -50%);
    -o-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
}
.emp-info-box .text{
    margin: 0 0 0 210px;
}
.emp-info-box .text h3{
    margin: 0 0 10px 0;
}
.emp-info-box .text h3 span{
    font-size: 14px;
    padding: 5px 10px;
    background-color: #c03b3b;
    color: #fff;
    text-transform: capitalize;
    line-height: normal;
}
.contact-info {
    list-style: none;
    margin: 25px 0 0 0;
}
.btn-link-wrp {
    position: absolute;
    bottom: 0;
    right: 0;
}
.btn-link-wrp a {
    text-transform: capitalize;
    color: #000;
    font-size: 20px;
}
.upload-file{
    float: left;
    width: 100%;
    position: relative;
}
.upload-file input[type="file"],
.upload-file a{
    position: absolute;
    top: 0;
    right: 0;
    width: 104px;
    height: 32px;
    background: #81b431;
}
.upload-file input[type="file"]{
    opacity: 0;
    z-index: 99;
    cursor: pointer;
}
.upload-file a{
    color: #fff;
    font-weight: 600;
    font-style: italic;
    text-align: center;
    line-height: 31px;
}
.selected-file{
    float: left;
    margin: 1px 0 0 0;
}
.btn-info {
    color: #fff;
    background-color: #81b431;
    border-color: #81b431;
}
.btn-black{
    background-color: #4e4e4e;
    color: #fff !important;
}
.btn-black:hover{
    background-color: #000;
}