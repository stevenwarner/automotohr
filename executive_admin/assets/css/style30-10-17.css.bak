@import url(https://fonts.googleapis.com/css?family=Open+Sans:400,300,300italic,400italic,600,600italic,700,700italic,800,800italic&subset=latin,cyrillic-ext,greek-ext,greek,vietnamese,latin-ext,cyrillic);
*{
    margin: 0;
    padding: 0;
    outline: none !important;
}
.clear{
    clear: both;
    font-size: 0;
    line-height: 0;
}
a{
    text-decoration: none;
}
html,
body {
	height:100%;
}
body{
    font-size: 14px;
    color: #000;
    line-height: 1.5;
    position: relative;
    overflow-x: hidden;
    font-family: 'Open Sans', sans-serif;
}
body h1, body h2, body h3, body h4, body h5, body h6{
    line-height: 1.3;
}
.page-title {
    margin: 0;
    font-size: 20px;
    color: #000;
    font-weight: 700;
    text-transform: capitalize;
}
.heading-title {
    float: left;
    width: 100%;
    margin: 0 0 20px 0;
    padding: 0 0 10px 0;
    border-bottom: 1px solid #eee;
}
.page-title i {
    margin: 0 15px 0 0;
}
.wrapper {
	min-height:100%;
	position:relative;
}
.header {
	background:#000;
	padding:10px;
}
.topRight {
    border-radius: 5px;
    color: #fff;
    float: right;
    font-size: 15px;
    margin: 10px 0 0 0;
    padding: 10px;
    background: #81b431;
    text-align: center;
}
.topRight a{
    color: #fff;
    text-decoration: underline;
}
.topRight a:hover{
    text-decoration: none;
}
.main {
	padding: 20px 0 100px 0;
}
.hr-login-section {
    margin: 65px auto;
    max-width: 600px;
}
.hr-box-wrap {
    border-radius: 0 0 5px 5px;
    box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
    padding: 25px 15px 15px 15px;
}
.hr-login-logo {
    float: left;
    margin: 0 0 20px;
    text-align: center;
    width: 100%;
}
.hr-login-box {
    float: left;
    width: 100%;
}
.fields-wrapper {
    float: left;
    position: relative;
    width: 100%;
}
.hr-login-box form ul {
    float: left;
    margin: 0;
    width: 100%;
}
.hr-login-box form ul li {
    float: left;
    list-style: outside none none;
    margin: 0 0 25px;
    width: 100%;
}
.field-icon {
    background-color: #81b431;
    border-radius: 50px 0 0 50px;
    color: #fff;
    font-size: 30px;
    height: 56px;
    left: 0;
    line-height: 51px;
    position: absolute;
    text-align: center;
    top: 0;
    width: 57px;
}
.form-fileds {
    background-color: #eee;
    border: medium none;
    border-radius: 50px;
    float: left;
    font-weight: 700;
    height: 56px;
    padding: 0 0 0 67px;
    width: 100%;
}
.executive-login-screen-heading{
    display: inline-block;
    width: 100%;
    text-align: center;
    background-color: #81b431;
    padding: 25px 10px;
    border-radius:5px 5px 0 0;
}
.executive-login-screen-heading h1{
    display: inline-block;
    margin: 0;
    font-size: 22px;
    text-transform: capitalize;
    color: #fff;
    font-weight: 700;
}
.hr-login-btn {
    background-color: #81b431;
    border: medium none;
    border-radius: 50px;
    color: #fff;
    display: inline-block;
    font-size: 22px;
    height: 64px;
    letter-spacing: 1px;
    line-height: 64px;
    padding: 0 25px;
    text-transform: uppercase;
    width: 100%;
}
.login-copyright {
    float: left;
    margin: 20px 0 0;
    text-align: center;
    width: 100%;
}
.login-copyright {
    color: #000;
    display: inline-block;
    font-size: 16px;
    font-weight: normal;
}

.hr-box-header {
    background: #eee none repeat scroll 0 0;
    border-radius: 5px 5px 0 0;
    float: left;
    margin: 0;
    overflow: hidden;
    padding: 9px;
    width: 100%;
}
.hr-box-header.hr-box-footer {
    border-radius: 0 0 5px 5px;
}
.table-outer {
    background-color: #e6e6e6;
    border-left: 5px solid #e6e6e6;
    border-right: 5px solid #e6e6e6;
    float: left;
    width: 100%;
}
.hr-promotions {
    background-color: #eee;
    box-shadow: 0 0 6px rgba(0, 0, 0, 0.3);
    float: left;
    margin: 20px 0;
    padding: 15px;
    width: 100%;
}
.no-data {
    background-color: #f2f7fa;
    border: 1px solid #d9ebf1;
    border-radius: 5px;
    clear: both;
    color: #728d9c;
    font-size: 14px;
    font-weight: 700;
    margin: 20px 0;
    padding: 12px 10px 10px;
    text-align: center;
    display: inline-block;
}
.hr-delete-btn, .hr-edit-btn {
    background-color: #ff6366;
    border: medium none;
    border-radius: 2px;
    color: #fff;
    display: inline-block;
    font-weight: 600;
    margin: 1px 4px;
    min-width: 60px;
    padding: 5px 10px;
    text-align: center;
}
.hr-edit-btn {
    background-color: #81b431 !important;
    color: #fff !important;
}
.table > thead > tr > th, 
.table > tbody > tr > th, 
.table > tfoot > tr > th, 
.table > thead > tr > td, 
.table > tbody > tr > td, 
.table > tfoot > tr > td {
	vertical-align: middle !important;
}
.information-box {
    float: left;
    margin: 10px 0;
    width: 100%;
}
.information-box .hr-box-header h2,
.hr-box-header h2{
    font-size: 16px;
    font-weight: 700;
    margin: 5px 0 0 0;
    float: left;
    text-transform: uppercase;
}
.info-row {
    float: left;
    padding: 15px 0;
    width: 100%;
}
.info-row ul {
    list-style: outside none none;
    margin: 0;
}
.info-row ul li {
    background-color: #fff;
    border-bottom: 1px solid #e0dfdf;
    float: left;
    padding: 5px;
    position: relative;
    width: 100%;
}
.info-row ul li:last-child {
    border: none;
}
.info-row ul li:nth-child(2n+1) {
    background-color: #f7f7f7;
}
.info-row ul li label {
    float: left;
    margin: 0;
    width: 40%;
}
.info-row ul li .text {
    float: left;
    width: 60%;
}
.table-title-heading {
    float: left;
    width: 100%;
    border-bottom: 2px solid #81b431;
    margin: 15px 00;
    color: #81b431;
}
.table-title-heading h1{
    font-size: 20px;
    font-weight: 600;
    color: #81b431;
}
.keep-me-loggedin{
    float: left;
    width: auto;
}
.forgot-password{
    float: right;
    width: auto;
}
.forgot-password a{
    text-decoration: underline;
    font-size: 13px;
    color: #000;
    font-style: italic;
}
.flash-message{
    float: left;
    width: 100%;
}
.error_message {
    color: #ff0000;
    display: inline-block;
    overflow: hidden;
    padding: 5px 0 0 10px;
}
.error {
    color: #ff0000;
    display: inline-block;
    width: 100%;
    font-weight: normal;
    border-color: #ff0000;
}
.error_message i {
    font-size: 18px;
    margin: 0 5px 0 0;
}
.footer {
	background:#000;
	width:100%;
	height:100px;
	position:absolute;
	bottom:0;
	left:0;
}
.copyright{
	display: table;
	height: 100px;
}
.copy-right-text{
	display: table-row;
}
.copy-right-text p {
    color: #fff;
    margin: 0;
    display: table-cell;
    vertical-align: middle;
}
.scrollable-area {
    float: left;
    /*height: 100%;*/
    max-height: 500px;
    overflow-y: auto;
    width: 100%;
}
.dotted-border {
    border-bottom: 1px dotted #a1a1a1;
    padding: 5px 0;
}
.Paid, .Unpaid {
    font-size: 16px;
    font-weight: 700;
}
.paid, .Paid {
    color: #81b431;
}
.unpaid, .Unpaid {
    color: red;
}
.autoheight{
    height: auto;
}
.auto-lineheight{
    line-height: normal;
}
.disabled-btn, .disabled-btn:hover {
    background-color: #ccc !important;
    color: #606060 !important;
    cursor: not-allowed !important;
}
.item-name-summary li {
    padding: 5px 0;
    list-style: none;
}
.table-striped tbody tr:nth-of-type(2n+2){
    background-color: #fff;
}
.heading-title .page-title {
    float: left;
}
.black-btn {
    background-color: #000;
    border: medium none;
    border-radius: 5px;
    color: #fff;
    display: inline-block;
    font-size: 14px;
    font-weight: 600;
    line-height: 24px;
    margin: 0 0 0 5px;
    padding: 4px 10px;
}
.table-custom{}
.add-new-company{}
